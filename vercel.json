{
  "version": 2,
  "builds": [
    {
      "src": "projects/timestamp-microservice/server.js",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["projects/timestamp-microservice/public/**"]
      }
    },
    {
      "src": "projects/request-header-parser/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["projects/request-header-parser/public/**"] }
    },
    {
      "src": "projects/url-shortener/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["projects/url-shortener/public/**"] }
    },
    {
      "src": "projects/exercise-tracker/server.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["projects/exercise-tracker/public/**"] }
    }
  ],
  "routes": [
    {
      "src": "/timestamp",
      "dest": "/projects/timestamp-microservice/server.js"
    },
    {
      "src": "/timestamp/(.*)",
      "dest": "/projects/timestamp-microservice/server.js"
    },
    {
      "src": "/request-header-parser",
      "dest": "/projects/request-header-parser/server.js"
    },
    {
      "src": "/request-header-parser/(.*)",
      "dest": "/projects/request-header-parser/server.js"
    },
    {
      "src": "/url-shortener",
      "dest": "/projects/url-shortener/server.js"
    },
    {
      "src": "/url-shortener/(.*)",
      "dest": "/projects/url-shortener/server.js"
    },
    {
      "src": "/exercise-tracker",
      "dest": "/projects/exercise-tracker/server.js"
    },
    {
      "src": "/exercise-tracker/(.*)",
      "dest": "/projects/exercise-tracker/server.js"
    }
  ]
}
